void setup(){
  
  size(500,400);
 
}
void draw(){ 
 if (mousePressed) {
  background(0);
  noStroke();
  fill(#1FC15E);
rect(100,25,300,50);
ellipse(250,200,300,300);
rect(100,325,300,50);
stroke(5);
  fill(0);
  ellipse(250,200,200,200);
 }
 else {
  background(0);
  noStroke();
  fill(#9E4EFA);
ellipse(250,200,250,250);
triangle(100,100,250,25,400,100); 
triangle(100,300,250,375,400,300);
  fill(0);
ellipse(250,200,175,175);
}
}
